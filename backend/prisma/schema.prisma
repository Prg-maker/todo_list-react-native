
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Profile{
  id String @id @default(uuid())
  name String
  github String?
  password String
}

model Tasks{
  id String @id @default(uuid())
  title String
  isChecking Boolean
}